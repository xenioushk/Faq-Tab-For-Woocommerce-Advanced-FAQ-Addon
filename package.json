{
  "name": "faq-tab-for-woocommerce",
  "version": "2.0.0",
  "description": "Convert existing FAQ posts in to WooCommerce product FAQ item.",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "dev": "wp-scripts start",
    "start:hot": "wp-scripts start --hot",
    "plugin-zip": "wp-scripts plugin-zip",
    "make-pot": "wp i18n make-pot . lang/faq-tab-for-woocommerce.pot --slug=faq-tab-for-woocommerce --domain=baf-faqtfw --exclude=node_modules,src",
    "make-json": "wp i18n make-json languages/ --no-purge",
    "lint:php": "vendor/bin/phpcs",
    "lint:phpn": "vendor/bin/phpcs -n",
    "lint:php:fix": "vendor/bin/phpcbf"
  },
  "author": "Mahbub Alam Khan <hkhan.cse@gmail.com> (https://bluewindlab.net/)",
  "license": "MIT",
  "dependencies": {
    "@wordpress/scripts": "24.6.0"
  }
}
